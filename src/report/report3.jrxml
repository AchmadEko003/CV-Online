	<queryString language="SQL">
		<![CDATA[SELECT
     PENCAPAIAN."NAMA" AS PENCAPAIAN_NAMA,
     PENCAPAIAN."DESKRIPSI" AS PENCAPAIAN_DESKRIPSI,
     PENGALAMAN."NAMA" AS PENGALAMAN_NAMA,
     PENGALAMAN."PERUSAHAAN" AS PENGALAMAN_PERUSAHAAN,
     PENGALAMAN."LAMA_BEKERJA" AS PENGALAMAN_LAMA_BEKERJA,
     USERS."NAMA" AS USERS_NAMA,
     USERS."EMAIL" AS USERS_EMAIL,
     USER_PROFILE."UMUR" AS USER_PROFILE_UMUR,
     USER_PROFILE."ALAMAT" AS USER_PROFILE_ALAMAT
FROM
     "CVWAH"."USERS" USERS INNER JOIN "CVWAH"."USER_PROFILE" USER_PROFILE ON USERS."ID" = USER_PROFILE."USERS_ID"
     INNER JOIN "CVWAH"."PENCAPAIAN" PENCAPAIAN ON USER_PROFILE."ID" = PENCAPAIAN."USER_PROFILE_ID"
     INNER JOIN "CVWAH"."PENGALAMAN" PENGALAMAN ON USER_PROFILE."ID" = PENGALAMAN."USER_PROFILE_ID"]]>
	</queryString>
