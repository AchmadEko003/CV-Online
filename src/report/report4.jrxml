	<queryString language="SQL">
		<![CDATA[SELECT
     KEAHLIAN."NAMA" AS KEAHLIAN_NAMA,
     KEAHLIAN."DESKRIPSI" AS KEAHLIAN_DESKRIPSI,
     RIWAYAT_PENDIDIKAN."NAMA" AS RIWAYAT_PENDIDIKAN_NAMA,
     RIWAYAT_PENDIDIKAN."JURUSAN" AS RIWAYAT_PENDIDIKAN_JURUSAN,
     RIWAYAT_PENDIDIKAN."ORGANISASI" AS RIWAYAT_PENDIDIKAN_ORGANISASI,
     USERS."NAMA" AS USERS_NAMA,
     USERS."EMAIL" AS USERS_EMAIL,
     USER_PROFILE."UMUR" AS USER_PROFILE_UMUR,
     USER_PROFILE."ALAMAT" AS USER_PROFILE_ALAMAT,
     RIWAYAT_PENDIDIKAN."IPK" AS RIWAYAT_PENDIDIKAN_IPK
FROM
     "CVWAH"."USERS" USERS INNER JOIN "CVWAH"."USER_PROFILE" USER_PROFILE ON USERS."ID" = USER_PROFILE."USERS_ID"
     INNER JOIN "CVWAH"."KEAHLIAN" KEAHLIAN ON USER_PROFILE."ID" = KEAHLIAN."USER_PROFILE_ID"
     INNER JOIN "CVWAH"."RIWAYAT_PENDIDIKAN" RIWAYAT_PENDIDIKAN ON USER_PROFILE."ID" = RIWAYAT_PENDIDIKAN."USER_PROFILE_ID"]]>
	</queryString>
