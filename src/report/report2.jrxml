	<queryString language="SQL">
		<![CDATA[SELECT
     BAHASA."NAMA" AS BAHASA_NAMA,
     SERTIFIKAT."NAMA" AS SERTIFIKAT_NAMA,
     SERTIFIKAT."LEMBAGA" AS SERTIFIKAT_LEMBAGA,
     USERS."NAMA" AS USERS_NAMA,
     USERS."EMAIL" AS USERS_EMAIL,
     USER_PROFILE."UMUR" AS USER_PROFILE_UMUR,
     USER_PROFILE."ALAMAT" AS USER_PROFILE_ALAMAT
FROM
     "CVWAH"."USERS" USERS INNER JOIN "CVWAH"."USER_PROFILE" USER_PROFILE ON USERS."ID" = USER_PROFILE."USERS_ID"
     INNER JOIN "CVWAH"."BAHASA" BAHASA ON USER_PROFILE."ID" = BAHASA."USER_PROFILE_ID"
     INNER JOIN "CVWAH"."SERTIFIKAT" SERTIFIKAT ON USER_PROFILE."ID" = SERTIFIKAT."USER_PROFILE_CON_FK"]]>
	</queryString>
