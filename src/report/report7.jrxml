	<queryString language="SQL">
		<![CDATA[SELECT
     APPLY."STATUS" AS APPLY_STATUS,
     LOWONGAN_PEKERJAAN."JUDUL" AS LOWONGAN_PEKERJAAN_JUDUL,
     LOWONGAN_PEKERJAAN."DESKRIPSI" AS LOWONGAN_PEKERJAAN_DESKRIPSI,
