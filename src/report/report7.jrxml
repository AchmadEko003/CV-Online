     "CVWAH"."LOWONGAN_PEKERJAAN" LOWONGAN_PEKERJAAN INNER JOIN "CVWAH"."APPLY" APPLY ON LOWONGAN_PEKERJAAN."ID" = APPLY."LOWONGAN_ID"
