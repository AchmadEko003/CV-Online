	<queryString language="SQL">
		<![CDATA[SELECT
     LOWONGAN_PEKERJAAN."JUDUL" AS LOWONGAN_PEKERJAAN_JUDUL,
     LOWONGAN_PEKERJAAN."DESKRIPSI" AS LOWONGAN_PEKERJAAN_DESKRIPSI,
     LOWONGAN_PEKERJAAN."REQUIREMENTS" AS LOWONGAN_PEKERJAAN_REQUIREMENT,
     LOWONGAN_PEKERJAAN."TANGGAL" AS LOWONGAN_PEKERJAAN_TANGGAL,
     LOWONGAN_PEKERJAAN."PERUSAHAAN" AS LOWONGAN_PEKERJAAN_PERUSAHAAN,
     USERS."NAMA" AS USERS_NAMA,
     USERS."EMAIL" AS USERS_EMAIL,
     USER_PROFILE."UMUR" AS USER_PROFILE_UMUR,
     USER_PROFILE."ALAMAT" AS USER_PROFILE_ALAMAT,
