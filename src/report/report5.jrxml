     USER_PROFILE."NO_TELEPON" AS USER_PROFILE_NO_TELEPON
FROM
     "CVWAH"."USERS" USERS INNER JOIN "CVWAH"."LOWONGAN_PEKERJAAN" LOWONGAN_PEKERJAAN ON USERS."ID" = LOWONGAN_PEKERJAAN."USER_ID"
     INNER JOIN "CVWAH"."USER_PROFILE" USER_PROFILE ON USERS."ID" = USER_PROFILE."USERS_ID"]]>
